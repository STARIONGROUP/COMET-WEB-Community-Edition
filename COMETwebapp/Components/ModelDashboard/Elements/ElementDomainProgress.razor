@using AntDesign

@if (this.ElementDefinitions.Any())
{
	var identifiedElements = Math.Round(((double)this.IdentifiedElements.Count() * 100)/ this.ElementDefinitions.Count());
	var otherElements = 100 - identifiedElements;
	<div class="container-fluid p-0">
		<div class="row m-0">
			<Progress Percent="otherElements" ></Progress>
			<Progress Percent="identifiedElements" StrokeColor="HaveChartData.WarningColor"></Progress>
		</div>

		<ElementTable Elements="@(this.IdentifiedElements)"/>
	</div>
}